import gradio as gr

teams = ['Cowboys', 'Eagles', 'Commanders', 'Giants']

def f(name):
    return len(name)


with gr.Blocks() as interface:
    team_dd = gr.Dropdown(teams, interactive = True ,label = "Select a Team")
    numLetters = gr .Number(label="Number of Letter in Team Name")
    team_dd.change(f, inputs=team_dd, outputs = [numLetters])



iface.launch()

